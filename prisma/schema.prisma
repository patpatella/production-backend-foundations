generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum JobStatus {
  OPEN
  COMPLETED
}

model Job {
  id             String    @id @default(uuid())
  posterId       String
  title          String
  description    String
  category       String
  suggestedPrice Float?
  status         JobStatus
  createdAt      DateTime  @default(now())
}
